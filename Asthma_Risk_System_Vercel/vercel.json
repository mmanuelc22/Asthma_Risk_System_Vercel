{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"  
    },
    {
      "src": "/api/predict",
      "dest": "/api/index.py"     
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"        
    }
  ]
}
